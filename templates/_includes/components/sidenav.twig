<nav class="c-sidenav l-column">
	<a class="c-logo" href="/">
		<span>A</span>
		<span>M</span>
	</a>
	<div class="c-sidenav__mobile js-mobile-nav">
		<span></span>
	</div>
	<ul class="c-sidenav__list js-sidenav">
		{% set entries = craft.entries.id('and, not 1, not 2, not 31') %}
		{% for entry in entries %}
			<li class="js-nav">
				<a class="c-sidenav__link {% if craft.app.request.getSegment(1) == entry.title %} active {% endif %}" href="{{ entry.url }}">{{ entry.title}}</a>
			</li>
		{% endfor %}
	</ul>
</nav>
